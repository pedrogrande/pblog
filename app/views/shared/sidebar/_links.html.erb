<div class="sidebar-module sidebar-module-inset">
    <h4><i class="fa fa-link fa-fw"></i> Elsewhere</h4>
    <ol class="list-unstyled">
      <% unless @user_settings.github == "" %>
        <li><i class="fa fa-github fa-fw fa-lg"></i> <%= link_to @user_settings.github, "http://github.com/" + @user_settings.github.to_s, target: '_blank' %></li>
      <% end %>
      <% unless @user_settings.twitter == "" %>
        <li><i class="fa fa-twitter fa-fw fa-lg"></i> <%= link_to @user_settings.twitter, "http://twitter.com/" + @user_settings.twitter.to_s, target: '_blank' %></li>
      <% end %>
      <% unless @user_settings.linked_in == "" %>
        <li><i class="fa fa-linkedin fa-fw fa-lg"></i> <%= link_to @user_settings.linked_in, "https://www.linkedin.com/in/" + @user_settings.linked_in.to_s, target: '_blank' %></li>
      <% end %>
      <% unless @user_settings.instagram == "" %>
        <li><i class="fa fa-instagram fa-fw fa-lg"></i> <%= link_to @user_settings.instagram, "https://www.instagram.com/" + @user_settings.instagram.to_s, target: '_blank' %></li>
      <% end %>
    </ol>
  </div>
